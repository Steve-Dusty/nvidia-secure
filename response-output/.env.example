# Emergency Response Agent Configuration
# Copy this file to .env and add your actual API keys

# NVIDIA NGC Configuration
NGC_API_KEY=nvapi-YOUR-API-KEY-HERE
NGC_MODEL_ENDPOINT=https://api.nvcf.nvidia.com/v2/nvcf/pexec/functions/YOUR-FUNCTION-ID

# VAPI (Voice API) Configuration
VAPI_API_KEY=vapi-YOUR-API-KEY-HERE

# Safety Settings
# WARNING: These are PLACEHOLDER numbers - NOT real 311/911
# DO NOT change these to real emergency numbers without proper authorization
PHONE_311_PLACEHOLDER=+1-555-0311-000
PHONE_911_PLACEHOLDER=+1-555-0911-000

# Model Parameters
URGENCY_SCORE_THRESHOLD=7.0
MODEL_TEMPERATURE=0.7
MODEL_MAX_TOKENS=1024

# EMS Configuration
AVERAGE_EMS_RESPONSE_TIME_MIN=3.67
AVERAGE_AMBULANCE_SPEED_MPH=25.0

# Logging
LOG_LEVEL=INFO
LOG_FILE=emergency_response.log
